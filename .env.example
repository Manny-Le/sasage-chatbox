# Environment Configuration Example
# Copy this file to .env and modify as needed

NODE_ENV=development

# API Base URLs for different environments
REACT_APP_API_BASE_URL_DEVELOPMENT=http://127.0.0.1:8000/api/v1
REACT_APP_API_BASE_URL_STAGING=http://staging-api.caster.com/api/v1
REACT_APP_API_BASE_URL_PRODUCTION=https://api.caster.com/api/v1

# Default API URL (fallback)
REACT_APP_API_BASE_URL=http://127.0.0.1:8000/api/v1

# App Configuration
REACT_APP_APP_NAME=Sasage Agent
REACT_APP_APP_VERSION=2.5 Pro
REACT_APP_COMPANY_NAME=Caster Company

# Feature Flags
REACT_APP_ENABLE_VOICE_INPUT=true
REACT_APP_ENABLE_FILE_UPLOAD=true
REACT_APP_ENABLE_VIDEO_CALL=true
REACT_APP_ENABLE_DEEP_SEARCH=true
REACT_APP_ENABLE_CANVAS=true
REACT_APP_ENABLE_IMAGE_UPLOAD=true
